{
  "name": "app-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf build lib",
    "build": "npm-run-all clean build:*",
    "build:server": "rimraf lib && cross-env NODE_ENV=production babel ./server --out-dir ./lib",
    "build:client": "rimraf build && cross-env NODE_ENV=production webpack",
    "test": "npm-run-all test:*",
    "test:server": "mocha --require server/test/setup.js --compilers js:babel-register './server/**/*.spec.js'",
    "test:client": "mocha --require client/test/setup.js --compilers js:babel-register './client/**/*.spec.js'",
    "test-watch:client": "npm run test:client -- -w",
    "test-watch:server": "nodemon -L --exec \"_mocha --require server/test/setup.js --compilers js:babel-register './server/**/*.spec.js' || true\"",
    "run-dev": "NODE_ENV=development nodemon --exec babel-node -w server  -- server/index.js"
  },
